	}


	if result_num < 0{
		//早期リターンなら何もせず終わる。
		logmes "検索早期リターン！当然０件だよ"
		return
	}
	
	kifu_update = 0

	search_result_sgflist = ""
	
	repeat result_num
		search_result_sgflist += search_result(0, cnt) + "\n"
	loop

	//search_result_sgflistに結果リスト、result_numに結果の数が入っている。

	gosub *filtering_and_make_listbox

